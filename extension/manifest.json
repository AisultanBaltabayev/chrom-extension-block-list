{
  "name": "Easy Block",
  "description": "",
  "version": "0.0.1",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_title": "Open Extension App"
  },
  "permissions": [
    "declarativeNetRequest",
    "alarms"
  ],
  "host_permissions": ["<all_urls>"],
